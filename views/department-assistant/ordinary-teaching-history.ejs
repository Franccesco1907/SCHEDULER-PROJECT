<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
  <%- include('../partials/head', {title: 'Histórico de solicitudes de plazas docencia ordinaria'}); %>
  <link rel="stylesheet" href="/assets/plugins/jquery-datatable/dataTables.bootstrap4.min.css" />
  
  <link rel="stylesheet" href="/assets/plugins/select2/select2.css" />
  <link rel="stylesheet" href="/light/assets/css/main.css">
  <link rel="stylesheet" href="/assets/plugins/dropzone/dropzone.css" />
  <!-- <link rel="stylesheet" href="/assets/plugins/bootstrap-select/css/bootstrap-select.css" /> -->
  <script src="https://kit.fontawesome.com/c1c34b92ef.js" crossorigin="anonymous"></script>
  <link
  rel="stylesheet"
  href="/assets/plugins/bootstrap-material-datetimepicker/css/bootstrap-material-datetimepicker.css"
/>
  <link rel="stylesheet" href="/assets/plugins/sweetalert/sweetalert.css" />
  
  <style>
    .request-less-hours-selected {
      background-color: #fff8b5 !important;
    }

    /* Chrome, Safari, Edge, Opera */
    input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
      -webkit-appearance: none;
      margin: 0;
    }

    /* Firefox */
    input[type="number"] {
      -moz-appearance: textfield;
    }

    .modal {
      padding-top: 250px !important;
    }
  </style>
</head>

<body class="theme-purple">
  <%- include('../partials/app/header') %>
  <%- include('../partials/app/topbar'); %>
  <%- include('../partials/app/leftsidebar', {user}); %>

  <section class="content">
    <div class="block-header">
      <div class="row">
        <div class="col-lg-7 col-md-6 col-sm-12">
          <h2>
            Histórico de solicitudes de plazas docencia ordinaria
            <small>Listado de solicitudes de plazas docencia ordinaria</small>
          </h2>
        </div>
        <div class="col-lg-5 col-md-6 col-sm-12">
          <ul class="breadcrumb float-md-right">
            <li class="breadcrumb-item">
              <a href="/"><i class="zmdi zmdi-home"></i>App</a>
            </li>
            <li class="breadcrumb-item"><a href="/">Gestión de docentes</a></li>
            <li class="breadcrumb-item active">Histórico de solicitudes de plazas docencia ordinaria</li>
          </ul>
        </div>
      </div>
    </div>
    <div class="container-fluid">
      
      <div class="row clearfix">
        <div class="col-lg-12">
          <div class="card">
            <div class="header">
              
              <h2><strong>Histórico de solicitudes de plazas docencia ordinaria</strong></h2>
            </div>
            <div class="body">           
              <input type="hidden" name="jwt" id="jwt" value="<%= jwt %>" />
              <div class="row clearfix pb-0 ">
                <div class="col-md-4">
                  <label for="beginning"
                    >Filtrar por fecha de inicio
                    </label
                  >
                  <div class="input-group">
                    <span class="input-group-addon">
                      <i class="zmdi zmdi-calendar"></i>
                    </span>
                    <input
                      type="text"
                      class="form-control date"
                      id="beginning"
                      placeholder="Elija la fecha de inicio"
                      data-dtp="dtp_wghlr"
                      required
                    />
                  </div>
                </div>
                <div class="col-md-4">
                  <label for="end"
                    >Filtrar por fecha de inicio
                    </label
                  >
                  <div class="input-group">
                    <span class="input-group-addon">
                      <i class="zmdi zmdi-calendar"></i>
                    </span>
                    <input 
                      type="text"
                      class="form-control date"
                      id="end"
                      placeholder="Elija la fecha fin"
                      data-dtp="dtp_wghlr"
                      required
                    />
                  </div>
                </div>
              </div>
              <div class="row pt-2 blocks">                                   
                <div class="col-md-4 d-inline-block">        
                    <label class="mb-1" for="selectSection">Sección </label>
                    <select class="form-control show-tick ms select2" name="selectSection" id="selectSection" data-placeholder="Seleccione una sección" style="height: 38px !important;" required>
                      <option></option>
                      <option value="-1">TODAS</option>
                      <% for (let section of sections ) { %>
                      <option value="<%= section.idSeccion %>">
                        <%= section.nombre %>
                      </option>
                      <% } %>
                    </select>
                </div>
              </div>
              <div class= "table-responsive">
                  <table id="request-less-hours-history-data-table" class="
                  table table-bordered table-striped table-hover
                  dataTable
                  js-exportable
                ">
                    <thead style="background-color: #167bc3; color:white">
                      <tr>
                        <!-- <th colspan="2" style="text-align: center !important;">Proceso</th> -->
                        <!-- <th colspan="4" style="text-align: center !important;">Descarga</th> -->
                      </tr>
                      <tr>
                        <!-- Procesos
                        <th >Fecha Inicio</th>
                        <th >Fecha Fin</th> -->
                        <!-- Descargas -->
                        <th>Fecha de envio</th>
                        <th >Sección</th>
                        <th >Cantidad de plazas solicitadas</th>
                        <th >Cantidad de plazas aprobadas</th>
                        <th >Cantidad de plazas aprobadas por DAP</th>
                      </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
              </div>
              
            </div>
          </div>
        </div>
      </div>
    </div>
       
    <!-- #END# Exportable Table -->
  </section>



  <%- include('../partials/app/scripts', {view: 'sb-ordinary-teaching-history'}); %>

  <!-- Jquery DataTable Plugin Js -->
  <script src="/light/assets/bundles/datatablescripts.bundle.js"></script>
  <script src="/assets/plugins/jquery-datatable/buttons/dataTables.buttons.min.js"></script>
  <script src="/assets/plugins/jquery-datatable/buttons/buttons.bootstrap4.min.js"></script>
  <script src="/assets/plugins/jquery-datatable/buttons/buttons.colVis.min.js"></script>
  <script src="/assets/plugins/jquery-datatable/buttons/buttons.html5.min.js"></script>
  <script src="/assets/plugins/jquery-datatable/buttons/buttons.print.min.js"></script>
  <script src="/assets/plugins/select2/select2.min.js"></script>
  <script src="/assets/plugins/bootstrap-notify/bootstrap-notify.js"></script>
  <script src="/light/assets/bundles/mainscripts.bundle.js"></script>
  <script src="/assets/plugins/momentjs/moment.js"></script>
  <script src="/assets/plugins/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js"></script>

  <script>
    var jwt = "<%= jwt %>";
    var sections = JSON.parse('<%-JSON.stringify(sections)%>');
  </script>

  <script src="/js/department-assistant/ordinary-teaching-history.js" type ="module"></script>

</body>

</html>